{
  "version": 2,
  "buildCommand": "echo 'No build step required'",
  "outputDirectory": "public",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/admin",
      "dest": "/admin/login.html"
    },
    {
      "src": "/admin/dashboard",
      "dest": "/admin/dashboard.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
